{
  "name": "babel-plugin-html-tag",
  "version": "1.4.0",
  "description": "Statically evaluates and minify tagged html`<..>` template literals into strings",
  "main": "index.js",
  "engines": {
    "node": ">=8.10"
  },
  "scripts": {
    "test": "node --expose-gc node_modules/jest/bin/jest --detectOpenHandles --detectLeaks --logHeapUsage",
    "typecheck": "tsc --p jsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinovyatkin/babel-plugin-html-tag.git"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "babel",
    "babel-plugin",
    "html",
    "template-literals",
    "common-tags",
    "es2015-tag",
    "template",
    "literal"
  ],
  "author": "Konstantin Vyatkin <tino@vtkn.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tinovyatkin/babel-plugin-html-tag/issues"
  },
  "homepage": "https://github.com/tinovyatkin/babel-plugin-html-tag#readme",
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@destinationstransfers/eslint-plugin": "2.9.62",
    "@types/babel__traverse": "7.0.8",
    "@types/node": "12.12.14",
    "eslint": "latest",
    "jest": "25.0.0",
    "jest-junit": "9.0.0",
    "typescript": "3.7.2"
  },
  "directories": {
    "test": "__tests__"
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node",
    "verbose": true,
    "coverageReporters": [
      "text",
      "json",
      "html",
      "cobertura"
    ]
  },
  "eslintConfig": {
    "plugins": [
      "@destinationstransfers"
    ],
    "extends": [
      "plugin:@destinationstransfers/recommended"
    ],
    "env": {
      "node": true
    },
    "parserOptions": {
      "sourceType": "script"
    }
  },
  "dependencies": {
    "html-minifier": "^4.0.0"
  }
}
