{
  "name": "babel-plugin-html-tag",
  "version": "1.3.0",
  "description": "Statically evaluates and minify tagged html`<..>` template literals into strings",
  "main": "index.js",
  "engines": {
    "node": ">=8.10"
  },
  "scripts": {
    "test": "node --expose-gc node_modules/jest/bin/jest --detectOpenHandles --detectLeaks --notify --logHeapUsage",
    "typecheck": "tsc --p jsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinovyatkin/babel-plugin-html-tag.git"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "babel",
    "babel-plugin",
    "html",
    "template-literals"
  ],
  "author": "Tino Vyatkin <tino@vtkn.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tinovyatkin/babel-plugin-html-tag/issues"
  },
  "homepage": "https://github.com/tinovyatkin/babel-plugin-html-tag#readme",
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@destinationstransfers/eslint-plugin": "2.4.6",
    "@types/babel__traverse": "7.0.6",
    "@types/node": "11.13.11",
    "eslint": "latest",
    "jest": "24.8.0",
    "jest-junit": "6.4.0",
    "typescript": "3.4.5",
    "weak": "1.0.1"
  },
  "directories": {
    "test": "__tests__"
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node",
    "verbose": true,
    "coverageReporters": [
      "text",
      "json",
      "html",
      "cobertura"
    ]
  },
  "eslintConfig": {
    "plugins": [
      "@destinationstransfers"
    ],
    "extends": [
      "plugin:@destinationstransfers/recommended"
    ],
    "env": {
      "node": true
    },
    "parserOptions": {
      "sourceType": "script"
    }
  },
  "dependencies": {
    "html-minifier": "^4.0.0"
  }
}
